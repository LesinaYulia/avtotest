time.sleep(3)
driver.find_element(By.CSS_SELECTOR, ".vue-dadata__suggestions-item").click()
time.sleep(8)
driver.find_element(By.ID, "phone").clear()
driver.find_element(By.ID, "phone").send_keys("9098885671")
driver.find_element(By.ID, "privacy").click()
time.sleep(3)
driver.find_element(By.ID, "privacy").click()
driver.find_element(By.XPATH, "//input[@type='file']").send_keys("C:\\Users\\User\\Desktop\\учеба\\дорожная карта.pdf")
time.sleep(6)
driver.find_element(By.XPATH, "//button[contains(.,\'Отправить\')]").click()
time.sleep(3)
